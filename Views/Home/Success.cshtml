@{
    Layout = null;
}
<!DOCTYPE html>
<html>

<head>
    <style>
        ::placeholder {
            font-size: 20px;
            font-weight: 100;
            text-align: center;
        }

        body {
            background-color: #111;
            font-family: monospace;
        }

        .container {
            align-items: center;
            display: flex;
            flex-direction: column;
            height: 13vh;
            justify-content: center;
            width: 100%;
        }

        h1 {
            color: PaleGoldenRod;
            font-family: 'Gambetta', serif;
            font-size: 80px;
            font-variation-settings: "wght" 311;
            letter-spacing: -3px;
            margin-bottom: 0.8rem;
            margin-right: 12px;
            margin-top: 10px;
            outline: none;
            text-align: center;
            text-transform: uppercase;
            transition: 700ms ease;
        }

        h5 {
            color: PaleGoldenRod;
            font-family: 'Gambetta', serif;
            font-size: 80px;
            font-variation-settings: "wght" 311;
            letter-spacing: 1px;
            margin-bottom: 0.8rem;
            margin-right: 12px;
            margin-top: 10px;
            outline: none;
            text-align: center;
            transition: 700ms ease;
        }

        h6 {
            color: PaleGoldenRod;
            font-family: 'Gambetta', serif;
            font-size: 80px;
            font-variation-settings: "wght" 311;
            letter-spacing: 0.5px;
            margin-bottom: 0.8rem;
            margin-right: 12px;
            margin-top: 10px;
            outline: none;
            text-align: start;
            transition: 700ms ease;
        }

        h1:hover {
            font-variation-settings: "wght" 582;
            letter-spacing: 1px;
        }

        h6:hover {
            font-variation-settings: "wght" 582;
            letter-spacing: 1px;
        }

        h5:hover {
            font-variation-settings: "wght" 582;
            letter-spacing: 3px;
        }

        h3 { transition: 700ms ease; }

        h3:hover {
            font-variation-settings: "wght" 582;
            letter-spacing: 3px;
        }


        #fnt { font-size: 14px; }

        .centered {
            background: #888;
            margin: 0 auto;
            margin-left: 5%;
            margin-right: 5%;
            width: 50%;
        }
        
    </style>

