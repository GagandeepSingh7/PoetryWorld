<!-- Write Publish And Content -->


<div class="row mx-0">
    <div class="col-4" style="background-color: #232323;">
        <form class="align-items-center">
            <div class="form-group text-center">
                <label for="exampleFormControlInput1" class=" "></label>
                <input type="text" class="form-control text-center rounded-0" id="exampleFormControlInput1"
                       placeholder="Write Your Title Here!">
            </div>


            <div class="form-group text-center">
                <label for="exampleFormControlTextarea1"></label>
                <textarea class="form-control text-center rounded-0" id="exampleFormControlTextarea1" rows="5"
                              placeholder="Write your poem here!"></textarea>
            </div>

            <button type="submit"
                    class="btn btn-secondary btn-lg btn-block w-100 fw-lighter bg-transparent rounded-0 border-0">
                Submit
            </button>

        </form>
    </div>


    <div class="col-4 p-0">
        <div class="card h-100 rounded-0 border-top-0  border-bottom-0">
